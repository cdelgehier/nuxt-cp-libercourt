{
  "name": "club-pongiste-libercourtois",
  "version": "1.0.0",
  "description": "Site vitrine du Club Pongiste Libercourtois",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.12.3",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:open": "nuxt dev --open",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"**/*.{vue,js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{vue,js,ts,json,md}\"",
    "typecheck": "vue-tsc --noEmit",
    "style:lint": "stylelint \"**/*.{css,scss,vue}\" --fix",
    "style:check": "stylelint \"**/*.{css,scss,vue}\"",
    "quality:check": "pnpm lint:check && pnpm format:check && pnpm typecheck && pnpm style:check",
    "quality:fix": "pnpm lint && pnpm format && pnpm style:lint",
    "precommit:setup": "./setup-precommit.sh"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.46",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@types/node": "^20.14.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.5.0",
    "nuxt": "^3.12.2",
    "postcss": "^8.5.6",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.4.5",
    "vue-tsc": "^2.0.21"
  },
  "dependencies": {
    "@nuxt/content": "^2.12.1",
    "@nuxt/image": "^1.7.0",
    "@nuxt/ui": "^2.17.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/nuxt": "^10.10.0",
    "googleapis": "^150.0.1",
    "jsdom": "^26.1.0",
    "pinia": "^2.1.7",
    "zod": "^3.25.67"
  },
  "keywords": [
    "ping-pong",
    "tennis-de-table",
    "club-sportif",
    "nuxt",
    "vue",
    "libercourtois"
  ],
  "author": "Club Pongiste Libercourtois",
  "license": "MIT"
}
