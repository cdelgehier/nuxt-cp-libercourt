{
  "name": "club-pongiste-libercourtois",
  "version": "1.0.0",
  "description": "Site vitrine du Club Pongiste Libercourtois",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{vue,js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{vue,js,ts,json,md}\"",
    "typecheck": "vue-tsc --noEmit",
    "style:lint": "stylelint \"**/*.{css,scss,vue}\" --fix",
    "style:check": "stylelint \"**/*.{css,scss,vue}\"",
    "quality:check": "npm run lint:check && npm run format:check && npm run typecheck && npm run style:check",
    "quality:fix": "npm run lint && npm run format && npm run style:lint",
    "precommit:setup": "./setup-precommit.sh"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@types/node": "^20.14.2",
    "eslint": "^9.5.0",
    "nuxt": "^3.12.2",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5",
    "vue-tsc": "^2.0.21"
  },
  "dependencies": {
    "@nuxt/content": "^2.12.1",
    "@nuxt/image": "^1.7.0",
    "@nuxt/ui": "^2.17.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/nuxt": "^10.10.0",
    "googleapis": "^150.0.1",
    "jsdom": "^26.1.0",
    "pinia": "^2.1.7",
    "zod": "^3.25.67"
  },
  "keywords": [
    "ping-pong",
    "tennis-de-table",
    "club-sportif",
    "nuxt",
    "vue",
    "libercourtois"
  ],
  "author": "Club Pongiste Libercourtois",
  "license": "MIT"
}
